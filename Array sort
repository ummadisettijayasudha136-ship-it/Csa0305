#include <stdio.h>

int main() {
    int arr[50],n,i,j,temp;
    printf("Enter size: "); scanf("%d",&n);
    for(i=0;i<n;i++) scanf("%d",&arr[i]);

    for(i=0;i<n-1;i++)
        for(j=i+1;j<n;j++)
            if(arr[i]>arr[j]) { temp=arr[i]; arr[i]=arr[j]; arr[j]=temp; }

    printf("Ascending: "); for(i=0;i<n;i++) printf("%d ",arr[i]);
    printf("\nDescending: "); for(i=n-1;i>=0;i--) printf("%d ",arr[i]);
    return 0;
}

