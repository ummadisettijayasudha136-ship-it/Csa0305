#include <stdio.h>

int main() {
    int arr[50], n, key, i, found = 0;
    printf("Enter number of students: ");
    scanf("%d", &n);

    printf("Enter registration numbers:\n");
    for (i = 0; i < n; i++)
        scanf("%d", &arr[i]);

    printf("Enter reg no to search: ");
    scanf("%d", &key);

    for (i = 0; i < n; i++) {
        if (arr[i] == key) {
            printf("Reg no %d found at position %d\n", key, i + 1);
            found = 1;
            break;
        }
    }
    if (!found)
        printf("Reg no %d not found\n", key);

    return 0;
}

