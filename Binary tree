#include <stdio.h>
#include <stdlib.h>
struct Node{ int data; struct Node* left; struct Node* right; };
struct Node* newNode(int d){ struct Node* n=(struct Node*)malloc(sizeof(struct Node)); n->data=d; n->left=n->right=NULL; return n; }
void inorder(struct Node* root){ if(root){ inorder(root->left); printf("%d ",root->data); inorder(root->right);} }
int main(){
    struct Node* root=newNode(1);
    root->left=newNode(2); root->right=newNode(3);
    root->left->left=newNode(4); root->left->right=newNode(5);
    printf("Inorder: "); inorder(root);
    return 0;
}
