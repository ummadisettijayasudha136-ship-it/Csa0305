#include <stdio.h>

int linear(int arr[],int n,int key){
    for(int i=0;i<n;i++) if(arr[i]==key) return i; return -1;
}
int binary(int arr[],int n,int key){
    int l=0,r=n-1,mid;
    while(l<=r){
        mid=(l+r)/2;
        if(arr[mid]==key) return mid;
        else if(arr[mid]<key) l=mid+1;
        else r=mid-1;
    }
    return -1;
}
int main(){
    int arr[]={10,20,30,40,50},n=5,key=30;
    printf("Linear search index=%d\n",linear(arr,n,key));
    printf("Binary search index=%d\n",binary(arr,n,key));
    return 0;
}
