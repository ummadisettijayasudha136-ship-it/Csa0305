#include <stdio.h>
#define V 4

int graph[V][V]={{0,1,1,0},{1,0,1,0},{1,1,0,1},{0,0,1,0}};
int visited[V];

void dfs(int start){
    printf("%d ",start);
    visited[start]=1;
    for(int i=0;i<V;i++)
        if(graph[start][i] && !visited[i])
            dfs(i);
}
int main(){
    printf("Valid path (DFS from 0): ");
    dfs(0);
    return 0;
}
