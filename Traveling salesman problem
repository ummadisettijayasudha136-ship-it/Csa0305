#include <stdio.h>
#include <limits.h>
#define V 4

int tsp(int graph[V][V], int v[], int pos, int n, int count, int cost) {
    if (count==n && graph[pos][0]) return cost+graph[pos][0];
    int ans = INT_MAX;
    for (int i=0;i<n;i++) {
        if (!v[i]) {
            v[i]=1;
            ans = (ans < tsp(graph,v,i,n,count+1,cost+graph[pos][i])) ? ans : tsp(graph,v,i,n,count+1,cost+graph[pos][i]);
            v[i]=0;
        }
    }
    return ans;
}

int main() {
    int graph[V][V]={{0,10,15,20},{10,0,35,25},{15,35,0,30},{20,25,30,0}};
    int v[V]={0}; v[0]=1;
    printf("Min cost: %d\n", tsp(graph,v,0,V,1,0));
    return 0;
}

