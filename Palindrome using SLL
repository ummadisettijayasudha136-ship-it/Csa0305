#include <stdio.h>
#include <stdlib.h>
#include <string.h>
struct Node{ char data; struct Node* next; };
int isPalindrome(struct Node* head,char str[]){
    int i=0; while(head){ str[i++]=head->data; head=head->next; }
    str[i]='\0';
    int l=0,r=i-1; while(l<r){ if(str[l]!=str[r]) return 0; l++; r--; }
    return 1;
}
int main(){
    struct Node* a=(struct Node*)malloc(sizeof(struct Node));
    a->data='m'; a->next=(struct Node*)malloc(sizeof(struct Node));
    a->next->data='a'; a->next->next=(struct Node*)malloc(sizeof(struct Node));
    a->next->next->data='m'; a->next->next->next=NULL;
    char str[100];
    if(isPalindrome(a,str)) printf("Palindrome\n"); else printf("Not Palindrome\n");
    return 0;
}
