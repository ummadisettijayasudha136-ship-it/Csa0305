#include <stdio.h>

int main() {
    int n, a = 0, b = 1, sum = 0, i, next;
    printf("Enter n: ");
    scanf("%d", &n);

    for (i = 1; i <= n; i++) {
        sum += a;
        next = a + b;
        a = b;
        b = next;
    }
    printf("Sum of Fibonacci series = %d\n", sum);
    return 0;
}

